<div id="<%= dom_id report %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= report.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Content:</strong>
    <%= report.content %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Published:</strong>
    <%= report.published %>
  </p>
</div>
